{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "web-app/.next",
  "builds": [
    {
      "src": "packages/cms-app/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "web-app/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/packages/cms-app/:path*"
    }
  ]
}
