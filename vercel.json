{
  "builds": [
    {
      "src": "packages/cms-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "web-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/packages/cms-app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web-app/$1"
    }
  ]
}